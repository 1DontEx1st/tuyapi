  },
  "homepage": "https://github.com/codetheweb/tuyapi#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "node-forge": "^0.7.1",
    "p-timeout": "^2.0.1",
    "retry": "^0.10.1"
